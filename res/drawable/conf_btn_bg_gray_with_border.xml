        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/uidic_global_color_c11_9" />
            <corners android:radius="4.0dip" />
            <stroke android:width="0.5dip" android:color="@color/uidic_global_color_c5" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/uidic_global_color_8_5" />
