<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="@color/cspace_bottom_disable" android:endColor="@color/cspace_bottom_disable" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/cspace_bottom_disable" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="@color/cspace_bottom_normal" android:endColor="@color/cspace_bottom_normal" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/cspace_bottom_normal" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="@color/cspace_bottom_hover" android:endColor="@color/cspace_bottom_hover" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>